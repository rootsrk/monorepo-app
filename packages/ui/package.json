{
  "name": "@rootsrk/ui",
  "version": "1.1.0",
  "private": true,
  "main": "./lib/index.ts",
  "types": "./lib/index.ts",
  "scripts": {
    "build": "storybook build",
    "i18n": "concurrently --max-processes 1 pnpm:i18n:extract pnpm:i18n:compile pnpm:i18n:pseudoLocale pnpm:lint:fix",
    "i18n:compile": "formatjs compile-folder --ast --format=smartling lang translations",
    "i18n:extract": "formatjs extract --format=smartling 'lib/**/*.ts*' --ignore='lib/**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --flatten",
    "i18n:pseudoLocale": "formatjs compile --pseudo-locale en-XA --ast --format=smartling lang/en.json --out-file translations/en-XA.json",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prepare": "npx playwright install chromium",
    "start": "storybook dev -p 6006 --no-open",
    "test": "concurrently pnpm:test:*",
    "test:storybook": "concurrently -k -s first -n 'Serve,Test', -c 'auto' 'pnpm exec http-server storybook-static --port 6006 --silent' 'pnpm exec wait-on tcp:6006 && pnpm exec test-storybook'",
    "test:typeCheck": "tsc"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "react-intl": "6.4.6",
    "tailwind-merge": "1.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.17",
    "@formatjs/cli": "6.1.5",
    "@formatjs/ts-transformer": "3.13.5",
    "@mdx-js/react": "2.3.0",
    "@rootsrk/tailwind-preset": "workspace:*",
    "@rootsrk/tsconfig": "workspace:*",
    "@storybook/addon-a11y": "7.4.0",
    "@storybook/addon-actions": "7.4.0",
    "@storybook/addon-backgrounds": "7.4.0",
    "@storybook/addon-console": "2.0.0",
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/addon-measure": "7.4.0",
    "@storybook/addon-outline": "7.4.0",
    "@storybook/addon-styling": "1.3.5",
    "@storybook/addons": "7.4.0",
    "@storybook/blocks": "7.4.0",
    "@storybook/channel-postmessage": "7.4.0",
    "@storybook/channel-websocket": "7.4.0",
    "@storybook/client-api": "7.4.0",
    "@storybook/jest": "0.2.2",
    "@storybook/manager-api": "7.4.0",
    "@storybook/preview-web": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-vite": "7.4.0",
    "@storybook/test-runner": "0.13.0",
    "@storybook/testing-library": "0.2.0",
    "@storybook/theming": "7.4.0",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "10.4.14",
    "axe-playwright": "1.2.3",
    "chromatic": "7.1.0",
    "concurrently": "8.2.0",
    "cssnano": "6.0.1",
    "eslint": "8.47.0",
    "eslint-plugin-rootsrk": "workspace:*",
    "eslint-plugin-storybook": "0.6.13",
    "http-server": "14.1.1",
    "playwright": "1.37.1",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.2",
    "storybook": "7.4.0",
    "storybook-react-intl": "2.0.2",
    "tailwindcss": "3.3.3",
    "type-fest": "4.1.0",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-dts": "3.5.3",
    "vite-plugin-svgr": "3.2.0",
    "wait-on": "7.0.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.2",
    "tailwindcss": "3.3.3",
    "zod": "3.21.4"
  }
}
